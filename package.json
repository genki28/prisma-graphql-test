{
  "name": "prisma-graphql-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate": "DATABASE_URL=file:./dev.db npx prisma migrate dev --name init --preview-feature",
    "seed": "DATABASE_URL=file:./dev.db npx prisma db seed --preview-feature",
    "generate": "npx prisma generate",
    "codegen": "graphql-codegen --config codegen.yml",
    "start": "cross-env NODE_ENV=local ts-node-dev --poll ./src/index.ts",
    "test": "cross-env NODE_ENV=testing jest",
    "test-migrate": "DATABASE_URL=file:./test.db npx prisma migrate dev --name init --preview-feature",
    "test-seed": "DATABASE_URL=file:./test.db npx prisma db seed --preview-feature",
    "test-generate": "npx prisma generate",
    "test-reset": "rm ./prisma/test.db"
  },
